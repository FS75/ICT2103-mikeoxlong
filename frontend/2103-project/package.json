{
  "name": "2103-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "update-fe": "npm install && npm update",
    "update-be": "cd ../../backend && npm install && npm update",
